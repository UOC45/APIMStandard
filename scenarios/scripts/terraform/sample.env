# .env file for Standard v2 deployment
AZURE_LOCATION='eastus2'
RESOURCE_NAME_PREFIX='apimv2'
ENVIRONMENT_TAG='dev'
APPGATEWAY_FQDN='apim.example.com'
CERT_TYPE='selfsigned'

# APIM SKU - Standard V2 or Basic V2 format: StandardV2_X or BasicV2_X where X is capacity
APIM_SKU_NAME='StandardV2_1'

# Optional: Certificate password (required only when CERT_TYPE='custom')
# CERT_PWD='your-certificate-password'